  dcterms:hasVersion "7.0"^^xsd:string ;
  lds:providesService :VirtuosoConductor , :VirtuosoAuthSPARQLEndpoint, :VirtuosoEndpoint;
  foaf:homepage <http://virtuoso.openlinksw.com/> .

:VirtuosoConductor 
  a  lds:StorageService ;
  lds:password "dba"^^xsd:string ;
  lds:serviceUrl <http://10.0.0.84:8890/conductor> ;
  lds:connectionString "jdbc:virtuoso://***REMOVED***:1111/"^^xsd:string;
  lds:user   "dba"^^xsd:string .

:VirtuosoAuthSPARQLEndpoint
  a       lds:SecuredSPARQLEndPointService,  void:Dataset , ontos:SPARQLEndpoint , ontos:DataSource ;
  rdfs:label "Workbench Auth" ;
  lds:password "generator"^^xsd:string ;
  lds:serviceUrl <http://***REMOVED***:8890/sparql-auth> ;
  void:sparqlEndpoint <http://***REMOVED***:8890/sparql-auth> ;
  lds:user   "generator"^^xsd:string .

:VirtuosoEndpoint
