# #######################################################################################
# Defined Roles
# #######################################################################################

:Administrator
      rdf:type ontos:Role ;
      rdfs:label "Administrator"@en .

:BasicUser
      rdf:type ontos:Role ;
      rdfs:label "Basic User"@en ;
      ontos:isDefault "true"^^xsd:boolean ;
      ontos:isNotLoggedIn "true"^^xsd:boolean .

:NotLoggedIn
      rdf:type ontos:Role ;
      rdfs:label "Not Logged In"@en .