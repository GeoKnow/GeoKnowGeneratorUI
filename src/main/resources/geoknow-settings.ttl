ldsc:GeoKnowGenerator
      rdf:type lds:StackComponent ;
      rdfs:comment "This resource contains all the configuration required by the application"^^xsd:string ;
      rdfs:label "GeoKnowGeneator"^^xsd:string ;
      lds:version "0.1"^^xsd:string ;
      lds:integrates :Virtuoso ;
      lds:outputGraph :SettingsGraph ;
      lds:serviceURL "http://localhost"^^xsd:string ;
      lds:useService :GeoKnowGeneratorService .

