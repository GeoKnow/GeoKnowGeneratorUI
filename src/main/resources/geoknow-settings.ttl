      lds:serviceUser "dba"^^xsd:string ;
      lds:version "0.1-SNAPSHOT"^^xsd:string ;
      lds:category "querying-and-exploration"^^xsd:string .

ldsc:Limes
      rdf:type lds:StackComponent ;
