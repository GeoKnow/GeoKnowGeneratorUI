@prefix : <http://generator.geoknow.eu/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix wv: <http://vocab.org/waiver/terms/norms> .        
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .

:GeoKnowGeneratorService a sd:Service;
    sd:endpoint <http://localhost:8890/sparql>;
    sd:supportedLanguage sd:SPARQL11Query ;
    sd:defaultDatasetDescription [
        a sd:Dataset;
        dcterms:title "GeoKnow Generator";
        dcterms:description "The GeoKnow Generator Service";
        sd:defaultGraph :defaultGraph;
        sd:namedGraph :settingsGraph;
        sd:namedGraph :schemaGraph;
     ];
.
:defaultGraph a sd:Graph, void:Dataset;
		dcterms:title "GeoKnow Generator SPARQL Endpoint Description";
		dcterms:description "Contains a copy of this SD+VoID file!";
.
:settingsGraph a sd:NamedGraph;
		sd:name :settingsGraph;
    sd:graph [
              a sd:Graph, void:Dataset;
              dcterms:title "This Graph contains the application specific settings and configurations";
              foaf:homepage <http://localhost/#/settings/>; 
          ];
.
:schemaGraph a sd:NamedGraph;
      sd:name :schemaGraph;
      sd:graph [
            a sd:Graph, void:Dataset;
            dcterms:title "Integrated LDS schema";
    	];
.
