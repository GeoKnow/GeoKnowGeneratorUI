# baseURI: http://linkeddata.org/integrated-stack-schema/

@prefix :        <http://linkeddata.org/integrated-stack-schema/> .
@prefix d2rq:    <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix dcmit:   <http://purl.org/dc/dcmitype/> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix foaf:    <http://xmlns.com/foaf/> .
@prefix gkgres:  <http://generator.geoknow.eu/resource/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema:  <http://schema.org/> .
@prefix sd:      <http://www.w3.org/ns/sparql-service-description#> .
@prefix void:    <http://rdfs.org/ns/void#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .


:     rdf:type owl:Ontology ;
      dcterms:contributor "Alejandra Garcia Rojas"^^xsd:string ;
      dcterms:descripton "Schema for integrating Software components from the Linked Data Stack"^^xsd:string ;
      dcterms:title "LDStack - Language Specification v0.1"^^xsd:string ;
      owl:versionInfo "0.1.0"^^xsd:string .

:API  rdf:type owl:Class ;
      rdfs:label "Service URL"^^xsd:string ;
      rdfs:subClassOf :SoftwareComponent .

:AnalysisService
      rdf:type owl:Class ;
      rdfs:comment "Represents the agent responsible for analysis."@en ;
      rdfs:label "Analysis Service"^^xsd:string ;
      rdfs:subClassOf :ComponentService .

:AuthoringService
      rdf:type owl:Class ;
      rdfs:comment "Represents the agent responsible for authoring."@en ;
      rdfs:subClassOf :ComponentService .

:CKAN
      rdf:type owl:Class ;
      rdfs:label "CKAN"^^xsd:string ;
      rdfs:subClassOf :DataSource .

:CleaningService
      rdf:type owl:Class ;
      rdfs:comment "Represents the agent responsible for storage."@en ;
      rdfs:label "Cleaning Service"^^xsd:string ;
      rdfs:subClassOf :ComponentService .

:ComponentCategory
      rdf:type rdfs:Class ;
      rdfs:label "Version"^^xsd:string .

:ComponentService
      rdf:type owl:Class ;
      rdfs:label "Version"^^xsd:string ;
      rdfs:subClassOf owl:Thing .

:DataSource
      rdf:type owl:Class ;
      rdfs:label "Data Source"^^xsd:string ;
      rdfs:subClassOf owl:Thing .

:Database
      rdf:type owl:Class ;
      rdfs:label "Database"^^xsd:string ;
      rdfs:subClassOf :DataSource .

:DatabaseType
      rdf:type owl:Class ;
      rdfs:label "Database port number"^^xsd:string ;
      rdfs:subClassOf owl:Thing .

:ExplorationService
      rdf:type owl:Class ;
      rdfs:comment "Represents the agent responsible for exploration."@en ;
      rdfs:subClassOf :ComponentService .

:ExtractionService
      rdf:type owl:Class ;
      rdfs:comment "Represents the agent responsible for extraction."@en ;
      rdfs:subClassOf :ComponentService .

:InterlinkingService
      rdf:type owl:Class ;
      rdfs:comment "Represents the agent responsible for interlinking."@en ;
      rdfs:subClassOf :ComponentService .

:MySql
      rdf:type :DatabaseType ;
      rdfs:label "MySql"^^xsd:string .

:Oracle
      rdf:type :DatabaseType ;
      rdfs:label "Oracle"^^xsd:string .

:PostGIS
      rdf:type :DatabaseType ;
      rdfs:label "PostGIS"^^xsd:string .

:PublicationService
      rdf:type owl:Class ;
      rdfs:comment "Represents the agent responsible for quality analysis."@en ;
      rdfs:subClassOf :ComponentService .

:QualityAnalysisService
      rdf:type owl:Class ;
      rdfs:comment "Represents the agent responsible for quality analysis."@en ;
      rdfs:subClassOf :ComponentService .

:QueryEndPoint
      rdf:type owl:Class ;
      rdfs:comment "Represents a query end point"@en ;
      rdfs:subClassOf :ExplorationService .

:SPARQLEndPoint
      rdf:type owl:Class ;
      rdfs:comment "Represents a Sparql query end point"@en ;
      rdfs:subClassOf :QueryEndPoint .

:SPARQLendpoint
      rdf:type owl:Class ;
      rdfs:label "SPARQL endpoint"^^xsd:string ;
      rdfs:subClassOf :DataSource .

:SoftwareComponent
      rdf:type owl:Class ;
      rdfs:label "Service URL"^^xsd:string ;
      rdfs:subClassOf owl:Thing .

:StackComponent
      rdf:type owl:Class ;
      rdfs:label "Software Component      "^^xsd:string ;
      rdfs:subClassOf :SoftwareComponent .

:StorageService
      rdf:type owl:Class ;
      rdfs:comment "Represents the agent responsible for storage."@en ;
      rdfs:subClassOf :ComponentService .

:TransformationService
      rdf:type owl:Class ;
      rdfs:comment "Represents the agent responsible for transformation."@en ;
      rdfs:subClassOf :ComponentService .

:dbHost
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Database ;
      rdfs:label "Service URL"^^xsd:string ;
      rdfs:range xsd:string .

:dbName
      rdf:type owl:DatatypeProperty ;
      rdfs:comment "Name of the database to connect"^^xsd:string ;
      rdfs:domain :Database ;
      rdfs:label "database"^^xsd:string ;
      rdfs:range xsd:string .

:dbPassword
      rdf:type owl:DatatypeProperty ;
      rdfs:domain
              [ rdf:type owl:Class ;
                owl:unionOf (:Database :ComponentService)
              ] ;
      rdfs:label "password"^^xsd:string ;
      rdfs:range xsd:string .

:dbPort
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Database ;
      rdfs:label "Database port number"^^xsd:string .

:dbType
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :Database ;
      rdfs:label "Database Type"^^xsd:string ;
      rdfs:range :DatabaseType .

:dbUserName
      rdf:type owl:DatatypeProperty ;
      rdfs:comment "If the component requires user/password authentication method (for instance a database)"^^xsd:string ;
      rdfs:domain
              [ rdf:type owl:Class ;
                owl:unionOf (:Database :ComponentService)
              ] ;
      rdfs:label "Service user"^^xsd:string ;
      rdfs:range xsd:string .

:endpoint
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :SPARQLendpoint ;
      rdfs:label "Service URL"^^xsd:string .

:integrates
      rdf:type owl:ObjectProperty ;
      rdfs:domain :StackComponent ;
      rdfs:label "integrates"^^xsd:string ;
      rdfs:range :StackComponent .

:jdbcDSN
      rdf:type owl:DatatypeProperty ;
      rdfs:comment "JDBC data source name." ;
      rdfs:label "JDBC DSN" .

:jdbcDriver
      rdf:type owl:DatatypeProperty ;
      rdfs:label "JDBC driver" .

:odbcDSN
      rdf:type owl:DatatypeProperty ;
      rdfs:comment "ODBC DSN" ;
      rdfs:label "ODBC data source name. Deprecated, use JDBC instead (with ODBC-JDBC bridge if necessary)." .

:providesService
      rdf:type owl:ObjectProperty ;
      rdfs:domain :SoftwareComponent ;
      rdfs:label "Version"^^xsd:string ;
      rdfs:range :ComponentService .

:requires
      rdf:type owl:ObjectProperty ;
      rdfs:domain :StackComponent ;
      rdfs:label "Service URL"^^xsd:string ;
      rdfs:range :StackComponent .

:serviceUrl
      rdf:type rdf:Property ;
      rdfs:domain :ComponentService ;
      rdfs:label "Service URL"^^xsd:string ;
      rdfs:range xsd:string .

:version
      rdf:type owl:DatatypeProperty ;
      rdfs:domain :SoftwareComponent ;
      rdfs:label "Version"^^xsd:string ;
      rdfs:range xsd:string .

dcterms:creator
      rdf:type owl:DatatypeProperty ;
      rdfs:domain void:Dataset ;
      rdfs:label "Service URL"^^xsd:string .

dcterms:description
      rdf:type owl:DatatypeProperty ;
      rdfs:domain void:Dataset ;
      rdfs:label "Service URL"^^xsd:string .

dcterms:source
      rdf:type owl:DatatypeProperty ;
      rdfs:domain void:Dataset ;
      rdfs:label "Service URL"^^xsd:string .

dcterms:title
      rdf:type owl:DatatypeProperty ;
      rdfs:comment "The name of the dataset."^^xsd:string ;
      rdfs:domain void:Dataset ;
      rdfs:label "Title"^^xsd:string .

void:Dataset
      rdf:type owl:Class ;
      rdfs:label "Service URL"^^xsd:string ;
      rdfs:subClassOf owl:Thing .

foaf:homepage
      rdf:type rdf:Property ;
      rdfs:domain :SoftwareComponent ;
      rdfs:label "Service URL"^^xsd:string .
